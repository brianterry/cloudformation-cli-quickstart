    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "cfn-rpdk": "https://github.com/eduardomourar/cloudformation-cli-typescript-plugin/releases/download/v0.1.0/cfn-rpdk-0.1.0.tgz",
    "node-fetch": "^3.0.0-beta.9"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
